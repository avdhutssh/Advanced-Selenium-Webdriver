<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SmokeTestSuite" verbose="1">
	
	<listeners>
		<listener class-name="com.herokuapp.theinternet.base.TestListener"></listener>
	</listeners>

	<test name="PositiveLogInTest">
		<classes>
			<class
				name="com.herokuapp.theinternet.loginpagetests.PositiveLogInTests" />
		</classes>
	</test>

	<test name="AlertsTests">
		<classes>
			<class name="com.herokuapp.theinternet.alertstests.AlertsTests" />
		</classes>
	</test>

	<test name="JSErrorTests">
		<classes>
			<class name="com.herokuapp.theinternet.jserrortests.JSErrorTests" />
		</classes>
	</test>

</suite>
